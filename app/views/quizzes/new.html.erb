 <%= form_for @quiz do |f| %>
  <% if @user.errors.any? %>
    <div class="error_messages">
      <h2>Form is invalid</h2>
      <ul>
        <% for message in @user.errors.full_messages %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <p>
  
    <%= f.text_field :title, placeholder: 'Title' %>

   
    <%= f.password_field :password, placeholder: 'password' %>
  </p>
  <p class="button"><%= f.submit %></p>
<% end %>